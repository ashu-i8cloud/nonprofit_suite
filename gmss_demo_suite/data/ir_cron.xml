<odoo>
  <record id="ir_cron_birthday_campaign" model="ir.cron">
    <field name="name">GMSS Birthday Email Campaign</field>
    <field name="model_id" ref="gmss_demo_suite.model_gmss_birthday_campaign"/>
    <field name="state">code</field>
    <field name="code">model.run_birthday_campaign(days_ahead=14)</field>
    <field name="interval_type">days</field>
    <field name="interval_number">1</field>
    <field name="active">True</field>
  </record>
</odoo>
